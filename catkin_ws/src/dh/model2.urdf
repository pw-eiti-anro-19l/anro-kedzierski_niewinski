
<robot name="dh">

<material name="blue">
    <color rgba="0 0 0.8 1"/>
</material>

<material name="white">
    <color rgba="1 1 1 1"/>
</material>


<link name="floor">
    	<visual>

      		<geometry>
        		<box size ="10 10 0"/>
     		 </geometry>
	<material name="white"/>
    	</visual>
</link>

<link name="link1">
    	<visual>

      		<geometry>
        		<cylinder length="1" radius="0.2"/>
     		 </geometry>
    	</visual>
</link>
<link name="link2">
   	 <visual>

      		<geometry>
        		<cylinder length="1" radius="0.2"/>
      		</geometry>
	<material name="blue"/>
    	</visual>
	
  </link>
<link name="link3">
    	<visual>

     		 <geometry>
       			 <cylinder length="1" radius="0.2"/>
      		</geometry>
	<material name="white"/>
    	</visual>
</link>

<joint name="joint1" type="revolute">
	<parent link="floor"/>
	<child link="link1"/>
	<origin xyz="0 0 0.5" rpy="0 0 0" />
	<limit effort="1000.0" lower="-1.57075" upper="1.57075" velocity="0.5"/>
	
</joint>


<joint name="joint2" type="revolute">
	<parent link="link1"/>
	<child link="link2"/>
	<origin xyz="0 0 0.5" rpy="-1.57 0 0" />
	<limit effort="1000.0" lower="-1.57075" upper="1.57075" velocity="0.5"/>
	
</joint>

<joint name="joint3" type="continuous">
	<parent link="link2"/>
	<child link="link3"/>
	<origin xyz="0 0 1" rpy="0 0 0" />
	
</joint>



</robot>
